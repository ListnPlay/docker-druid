                 ]
               },
               "dimensionsSpec" : {
                 "dimensions" : [
                   "keywords",
                   "genres",
                   "similarArtists",
                   "pipeline",
                   "platform",
                   "consumerToken",
                   "consumer_ip",
                   "consumer_gender",
                   "consumer_birth",
                   "consumer_continent",
                   "consumer_country",
                   "consumer_region",
                   "consumer_city",
                   "api_key",
                   "app_name"
                 ]
               }
             }
           },
           "metricsSpec" : [
             {
               "type" : "count",
               "name" : "count"
             },
             {
               "name" : "amount_sum",
               "type" : "longSum",
               "fieldName" : "amount"
             }
           ]
         },
         "tuningConfig" : {
           "type" : "realtime",
           "intermediatePersistPeriod" : "PT10S",
           "windowPeriod" : "PT10S",
           "maxRowsInMemory" : 75000
